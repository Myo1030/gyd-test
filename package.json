{
  "name": "my-new-project",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "tsc && npm-run-all --parallel start:expo build:watch",
    "start:expo": "expo start",
    "android": "tsc && expo start --android",
    "ios": "tsc && expo start --ios",
    "eject": "tsc && expo eject",
