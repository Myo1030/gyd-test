    "react-native-elements": "^1.0.0-beta5",
    "react-navigation": "^2.16.0"
  },
  "devDependencies": {
    "@types/expo": "^27.0.10",
    "@types/jest": "^23.3.2",
    "@types/react-native-elements": "^0.18.0",
    "@types/react-navigation": "2.0.22",
    "expo-cli": "2.1.3",
    "npm-run-all": "4.1.3",
    "jest-expo": "30.0.0",
    "tslint": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "3.0.3"
  }
